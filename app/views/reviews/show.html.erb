<p id="notice"><%= notice %></p>

<div class="container review-container">
	<div review-info-content">
		<h2><%= @review.movie_title %></h2>
		<h5><%= @review.review_date %></h5>
		<p><%= @review.rating %></p>
		<p><%= @review.comment %></p>
		<p><%= @review.user_email %></p>
		<%= link_to "Back to review list", review_list_path %>
	</div>
</div>

